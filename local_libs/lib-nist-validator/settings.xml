<?xml version="1.0" encoding="UTF-8" ?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <activeProfiles>
        <activeProfile>nist nexus</activeProfile>
    </activeProfiles>

    <profiles>
        <profile>
            <id>nist nexus</id>
            <repositories>
              <repository>
                  <id>nist nexus</id>
                  <name>NIST Nexus Repository</name>
                  <url>https://hit-nexus.nist.gov</url>
              </repository>
            </repositories>
        </profile>
    </profiles>

    <servers>
        <server>
            <id>nist nexus</id>
        </server>
    </servers>
</settings>